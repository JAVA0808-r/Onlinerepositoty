public class Point {
    private double x;
    private double y;
    private double z;

    // Constructor - obyekt yaratilganda x,y,z qiymatlari beriladi
    public Point(double x, double y, double z) {
        this.x = x;
        this.y = y;
        this.z = z;
    }

    // Getter va Setter metodlari (Encapsulation)
    public double getX() {
        return x;
    }

    public void setX(double x) {
        this.x = x;
    }

    public double getY() {
        return y;
    }

    public void setY(double y) {
        this.y = y;
    }

    public double getZ() {
        return z;
    }

    public void setZ(double z) {
        this.z = z;
    }

    // Nuqta koordinatalarini chiqarish
    public void displayCoordinates() {
        System.out.println("Point coordinates: (" + x + ", " + y + ", " + z + ")");
    }
}
